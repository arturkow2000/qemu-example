{
    "version": "2.0.0",
    "tasks": [
        {
            "group": "build",
            "label": "qemu-example: build",
            "type": "cargo",
            "command": "build",
            "args": [
                "--target",
                "armv7a-none-eabi"
            ],
            "problemMatcher": "$rustc",
            "presentation": {
                "echo": true,
                "reveal": "silent",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            },
            "env": {
                "DEFMT_LOG": "debug"
            }
        }
    ]
}
